{
  "name": "Snax",
  "short_name": "SNAX",
  "version": "0.1.0",
  "manifest_version": 2,
  "author": "https://snax.com",
  "description": "Snax extension",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": ["background.bundle.js"],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "19": "icon16.png",
      "38": "icon32.png",
      "128": "icon128.png"
    },
    "default_title": "Snax",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["http://localhost/*", "https://snax.one/*"],
      "js": ["contentscript.bundle.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "web_accessible_resources": ["injected.bundle.js"],
  "permissions": ["storage", "clipboardWrite", "activeTab", "notifications"]
}
