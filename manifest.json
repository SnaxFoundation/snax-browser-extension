{
  "name": "Mints",
  "short_name": "MNT",
  "version": "0.1.0",
  "manifest_version": 2,
  "author": "https://mints.com",
  "description": "Mints extension",
  "icons": {
    "16": "favicon.png",
    "128": "favicon.png"
  },
  "background": {
    "scripts": [
      "background.bundle.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "19": "favicon.png",
      "38": "favicon.png"
    },
    "default_title": "Mints",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "file://*/*",
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "contentscript.bundle.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "permissions": [
    "storage",
    "unlimitedStorage",
    "clipboardWrite",
    "http://localhost:8545/",
    "https://*.infura.io/",
    "activeTab",
    "webRequest",
    "*://*.eth/",
    "notifications"
  ]
}